#include "main.h"

/**
 * print_rev - prints a string in reverse, followed by a new line
 * @s: string to be printed
 */

void print_rev(char *s)
{
  int a, b, length;
  while(s[a] != '\0')
  {
    a++;
  }
  length = a;
  for(b = length - 1; b >= 0; b--)
  {
    _putchar(s[b]);
  }
  _putchar('\n');
}
